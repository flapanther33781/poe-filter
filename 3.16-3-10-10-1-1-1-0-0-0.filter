    Rarity Rare
    ItemLevel >= 0
    LinkedSockets = 4
    Width = 2
    Height = 2
    SetFontSize 40
    SetTextColor 0 0 0 255
    SetBackgroundColor 28 236 4 255     # BACKGROUNDCOLOR GREEN
    PlayAlertSound 8 300
    MinimapIcon 0 Green Triangle
